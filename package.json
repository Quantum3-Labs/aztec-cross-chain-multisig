{
  "name": "aztec-wormhole-multisig",
  "version": "1.0.0",
  "description": "**Foundry is a blazing fast, portable and modular toolkit for Ethereum application development written in Rust.**",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "aztec:compile": "cd packages/aztec && yarn compile",
    "aztec:codegen": "cd packages/aztec && yarn codegen",
    "aztec:gen-key": "cd packages/aztec && yarn gen-key",
    "aztec:deploy": "cd packages/aztec && yarn deploy",
    "aztec:test": "cd packages/aztec && yarn test-multisig-public",
    "aztec:all": "yarn aztec:compile && yarn aztec:codegen && yarn aztec:gen-key && yarn aztec:deploy && yarn aztec:test",
    "arbitrum:build": "forge -C packages/arbitrum build",
    "arbitrum:test": "forge -C packages/arbitrum make all",
    "arbitrum:deploy": "forge -C packages/arbitrum script script/DeployVault.s.sol",
    "arbitrum:deploy:donation": "cd packages/arbitrum && make deploy-donation",
    "arbitrum:deploy:vault": "cd packages/arbitrum && make deploy-vault",
    "arbitrum:register": "cd packages/arbitrum && make register",
    "arbitrum:verify": "cd packages/arbitrum && make verify",
    "arbitrum:all": "cd packages/arbitrum && make all"
  },
  "dependencies": {
    "@types/node": "^24.5.2",
    "dotenv": "^17.2.2",
    "ethers": "^6.15.0"
  }
}
